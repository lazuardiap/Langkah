<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.newshelter.langkah">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Langkah">
        <activity android:name=".ui.profile.ChangePasswordActivity"></activity>
        <activity android:name=".ui.profile.EditProfileActivity" />
        <activity android:name=".ui.appointment.AppointmentActivity" />
        <activity android:name=".ui.room.RoomDetailActivity" />
        <activity android:name=".ui.polyclinics.PolyclinicDetailsActivity" />
        <activity android:name=".ui.doctors.DoctorDetailsActivity" />
        <activity android:name=".ui.hospital.HospitalDetailActivity" />
        <activity android:name=".AppActivity" />
        <activity
            android:name=".LandingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Langkah">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>